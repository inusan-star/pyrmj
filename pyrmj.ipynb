    "\n",
    "game = Game(rule({\"場数\": 1, \"連荘方式\": 0, \"トビ終了あり\": False, \"延長戦方式\": 0}))\n",
    "observation = game.reset(chiicha=None, save_flag=False)\n",
    "\n",
    "while not game.done():\n",
    "    actions = {0: {}, 1: {}, 2: {}, 3: {}}\n",
    "    observation = game.step(actions)\n"
   ]
